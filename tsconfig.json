{
    "version": "1.4.0",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "compileOnSave": false,
    "filesGlob": [
        "./src/typescript/*.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "./src/typescript/Main.ts"
    ]
}
